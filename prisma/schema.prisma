generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Feedback {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Newsletter {
  id    Int    @id @default(autoincrement())
  email String @unique
}

model Quote {
  id           Int      @id @default(autoincrement())
  name         String
  email        String
  businessName String
  projectType  String
  message      String
  phone        String
  date         DateTime @default(now())
}
